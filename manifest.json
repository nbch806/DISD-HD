{
  "manifest_version": 2,

  "name": "Think Help Desk",

  "description": "This Chrome Extension is built for Think Help Desk Clients, if you do not have Think Help Desk, please contact sales@thinkhelpdesk.",

  "version": "0.0.0.1",

  "background": {
      "scripts": [
        "js/jquery.min.js",
        "js/background.js"
      ],
      "persistent": false
  },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage",
    "alarms",
    "desktopCapture"
  ],

  "options_page": "options.html",

  "icons": {
    "200": "img/icon.png"
  }
}
